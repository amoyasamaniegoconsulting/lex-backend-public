{
  "functions": {
    "/**.py": {
      "runtime": "@vercel/python@4.3.0",
      "excludeFiles": "{.next,.git,node_modules}/**"
    }
  },
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ]
}
